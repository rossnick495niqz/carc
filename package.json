{
    "name": "auto-import-calculator",
    "private": true,
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "test": "vitest run",
        "build": "tsc && vite build && npm run data:copy",
        "data:fetch": "npx tsx scripts/data/fetch_raw.ts",
        "data:build": "npx tsx scripts/data/build_catalog.ts && npx tsx scripts/data/build_parsed.ts",
        "data:copy": "mkdir -p public/data/official && cp data/official/parsed/*.json public/data/official/ && cp data/catalog.json public/data/catalog.json",
        "data:all": "npm run data:fetch && npm run data:build && npm run data:copy",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview"
    },
    "dependencies": {
        "@capacitor/android": "^7.0.0",
        "@capacitor/core": "^7.0.0",
        "@capacitor/ios": "^7.0.0",
        "@tanstack/react-query": "^5.0.0",
        "@types/uuid": "^10.0.0",
        "body-scroll-lock": "^4.0.0-beta.0",
        "clsx": "^2.0.0",
        "framer-motion": "^10.16.4",
        "html-to-image": "^1.11.11",
        "idb-keyval": "^6.2.1",
        "jspdf": "^2.5.1",
        "lucide-react": "^0.292.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "tailwind-merge": "^2.0.0",
        "uuid": "^13.0.0",
        "zod": "^3.22.0",
        "zustand": "^4.4.0"
    },
    "devDependencies": {
        "@capacitor/assets": "^3.0.5",
        "@capacitor/cli": "^7.0.0",
        "@types/body-scroll-lock": "^3.1.2",
        "@types/react": "^18.2.15",
        "@types/react-dom": "^18.2.7",
        "@types/xmldom": "^0.1.33",
        "@vitejs/plugin-react": "^4.0.3",
        "autoprefixer": "^10.4.14",
        "jsdom": "^22.1.0",
        "postcss": "^8.4.27",
        "tailwindcss": "^3.3.3",
        "tsx": "^3.12.0",
        "typescript": "^5.0.2",
        "vite": "^4.4.5",
        "vite-plugin-pwa": "^1.2.0",
        "vitest": "^0.34.0",
        "workbox-core": "^7.4.0",
        "workbox-window": "^7.4.0",
        "xmldom": "^0.6.0"
    }
}
